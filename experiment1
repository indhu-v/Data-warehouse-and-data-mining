num=as.integer(readline(prompt("enter a number:"))
if((num%%2)==0){
print("number is even")
}else{
print("number is odd")
}
num1=as.integer(readline(prompt("enter a number1:"))
num2=as.integer(readline(prompt("enter a number2:"))
num3=num1+num2
print(num3)
n1=as.integer(readline(prompt("enter a number1:"))
n2=as.integer(readline(prompt("enter a number2:"))
n3=n1-n2
print(n3)
